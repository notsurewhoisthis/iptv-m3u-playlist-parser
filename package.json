{
  "name": "iptv-parser",
  "version": "0.1.0",
  "private": false,
  "description": "Robust IPTV M3U/M3U8 playlist parser with IPTV-specific tags (tvg-*, group-title, EXTGRP, EXTVLCOPT, KODIPROP, catchup, etc.).",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "iptv-parse": "bin/iptv-parse.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx examples/parse-file.ts",
    "test": "node --test",
    "fmt": "prettier -w ."
  },
  "keywords": [
    "iptv",
    "m3u",
    "m3u8",
    "playlist",
    "parser",
    "xmltv",
    "epg"
  ],
  "author": "",
  "license": "",
  "devDependencies": {
    "@types/node": "^22.7.5",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}